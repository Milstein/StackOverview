{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "StackOverview",
  "author": "Jason Wang",
  "description": "Quick stats on StackOverflow and StackExchange posts from Google search results",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*", "https://www.google.com/*"],
      "css": ["style.css"],
      "js": ["index.js"]
    }
  ],
  "permissions": [
    "http://*.stackoverflow.com/*",
    "https://*.stackoverflow.com/*",
    "http://*.stackexchange.com/*",
    "https://*.stackexchange.com/*",
    "tabs"
   ],
   "icons": {
       "48": "icons/so48trans.png",
       "128": "icons/so128trans.png"
   }
}
